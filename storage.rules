
rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow users to read and write files only in their own folder.
    // The path is structured as 'users/{userId}/{allPaths=**}'
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
